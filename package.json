{
  "name": "my-v0-project",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "dev": "next dev",
    "check:src-structure": "node scripts/check-src-structure.mjs",
    "lint": "node scripts/check-src-structure.mjs",
    "lint:structure": "node scripts/check-src-structure.mjs",
    "check:regression": "./scripts/regression-guards.sh",
    "lint:eslint": "eslint .",
    "test": "NODE_OPTIONS='--max-old-space-size=2048' vitest run",
    "test:coverage": "NODE_OPTIONS='--max-old-space-size=2048' vitest run --coverage",
    "test:automate": "node scripts/test-coverage-automator.js",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:smoke": "playwright test e2e/launch-smoke.spec.ts --project=chromium --reporter=github",
    "test:e2e:core": "playwright test e2e/marketplace/browse.spec.ts e2e/example.spec.ts e2e/diagnostic.spec.ts --project=chromium --timeout=30000",
    "test:e2e:extended": "playwright test --ignore e2e/marketplace/browse.spec.ts --ignore e2e/example.spec.ts --ignore e2e/diagnostic.spec.ts",
    "test:unit:legacy": "NODE_OPTIONS='--max-old-space-size=2048' vitest run",
    "db:migrate:test": "NODE_ENV=test dotenv -e .env.test -- npx prisma migrate deploy",
    "db:reset:test": "NODE_ENV=test bash scripts/reset-test-db.sh",
    "db:seed:test": "NODE_ENV=test dotenv -e .env.test -- npx ts-node prisma/seed-e2e.ts",
    "e2e": "NODE_ENV=test dotenv -e .env.test -- playwright test e2e/auth.e2e.spec.ts",
    "e2e:headed": "NODE_ENV=test dotenv -e .env.test -- playwright test e2e/auth.e2e.spec.ts --headed",
    "e2e:debug": "NODE_ENV=test dotenv -e .env.test -- playwright test e2e/auth.e2e.spec.ts --debug",
    "e2e:report": "NODE_ENV=test dotenv -e .env.test -- playwright test e2e/auth.e2e.spec.ts && playwright show-report",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "start": "next start",
    "dev:orchestrator": "tsx scripts/dev-orchestrator.ts",
    "dev:all": "bash scripts/dev-all.sh",
    "flow:gemini": "tsx scripts/run-gemini-flow.ts",
    "drizzle:generate": "drizzle-kit generate",
    "db:sync": "SUPABASE_DB_URL=${SUPABASE_DB_URL} pnpm dlx supabase db diff --file auto-update --db-url \"$SUPABASE_DB_URL\" && SUPABASE_DB_URL=${SUPABASE_DB_URL} pnpm dlx supabase db push --db-url \"$SUPABASE_DB_URL\" && pnpm drizzle:generate",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
